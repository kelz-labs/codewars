#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/run/media/haikel/1f7b5dc8-2a53-4740-b7bb-b0fbc05dfb45/Exercise/learn/Javascript_Course/codewars/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/run/media/haikel/1f7b5dc8-2a53-4740-b7bb-b0fbc05dfb45/Exercise/learn/Javascript_Course/codewars/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typescript@5.0.4/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../../typescript@5.0.4/node_modules/typescript/bin/tsc" "$@"
fi
